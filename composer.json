{
	"name": "codekandis/checklist",
	"description": "`codekandis/checklist` is a simple checklist tool.",
	"version": "dev-development",
	"keywords": [
		"php",
		"checklist",
		"tool"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/checklist/issues",
		"docs": "https://github.com/codekandis/checklist/README.md",
		"source": "https://github.com/codekandis/checklist/tree/master"
	},
	"config": {
		"allow-plugins": {
			"php-http/discovery": true
		}
	},
	"require": {
		"php": "^7.4"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^3"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\CheckList\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\CheckList\\Tests\\": [
				"tests/"
			]
		}
	}
}
