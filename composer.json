{
	"name": "codekandis/checklist",
	"description": "`codekandis/checklist` is a simple checklist tool.",
	"version": "dev-development",
	"keywords": [
		"php",
		"checklist",
		"tool"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/checklist/issues",
		"docs": "https://github.com/codekandis/checklist/README.md",
		"source": "https://github.com/codekandis/checklist/tree/master"
	},
	"config": {
		"allow-plugins": {
			"php-http/discovery": true
		}
	},
	"require": {
		"php": "^7.4",
		"codekandis/authentication": "^0",
		"codekandis/configurations": "^0",
		"codekandis/jotunheim": "^0",
		"codekandis/persistence": "^0",
		"codekandis/sentry-client": "^1",
		"codekandis/session": "^0",
		"codekandis/tiphy": "^0",
		"codekandis/tiphy-authentication-integration": "^0",
		"codekandis/tiphy-persistence-integration": "^0",
		"codekandis/tiphy-sentry-client-integration": "^0",
		"codekandis/tiphy-session-integration": "^0",
		"codekandis/validators": "^0",
		"fortawesome/font-awesome": "^6",
		"php-http/message-factory": "^1",
		"twbs/bootstrap": "^5"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^3"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\CheckList\\": [
				"config/",
				"public/",
				"src/"
			],
			"CodeKandis\\CheckList\\Configurations\\Plain\\": [
				"config/",
				"src/Configurations/Plain"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\CheckList\\Tests\\": [
				"tests/"
			]
		}
	}
}
